  </setupTask>
  <setupTask
      xsi:type="setup:VariableTask"
      name="gitlab.user.id"
      label="Gitlab user ID">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="target">
        <value>userID</value>
      </detail>
    </annotation>
    <description>The Gitlab user ID</description>
  </setupTask>
  <setupTask
      xsi:type="setup:VariableTask"
      name="gitlab.remoteURIs"
      label="Gitlab repository">
    <choice
        value="git@@{remoteURI}:development/algotrader.git"
        label="GIT SSH (requires pre ssh-keys)"/>
    <choice
        value="https://@{remoteURI}/gitlab/development/algotrader.git"
        label="HTTPS (with user/pass)"/>
    <choice
        value="git@@{remoteURI}:ssampaoli/algotrader.git"
