      id="git.clone"
      location="${workspace.location/git}"
      remoteURI="repo.algotrader.ch">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>gitlab.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>${scope.project.label} Gitlab repository</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="maven:MavenImportTask"
      id=""
      projectNameTemplate="">
    <sourceLocator
        rootFolder="${git.clone.location/eclipse}"/>
  </setupTask>
